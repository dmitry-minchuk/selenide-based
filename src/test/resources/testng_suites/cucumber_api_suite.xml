<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Cucumber API" annotations="JDK">

    <parameter name="retry_count" value="2"/>
    <parameter name="env" value="STAG"/>
    <parameter name="cron" value="40 2 * * 1-5"/>
    <parameter name="enableVnc" value="false"/>

    <test verbose="1" name="Tests by cucumber">
        <classes>
            <class name="tests.cucumber.CucumberApiTest">
            </class>
        </classes>
    </test>
</suite>